let a = 5
let b = 3

let adicao  = a + b
let sub = a - b
let mult = a * b
let divisao = a / b
let cubo = a ** b
let resto1 = a % b;
let resto2 = b % a;

let par = 4 % 2
let impar = 5 % 2
 
console.log(adicao)
console.log(sub)
console.log(mult)
console.log(divisao)
console.log(cubo)
console.log(resto1)
console.log(resto2)
console.log("---")

console.log("PAR: " + par)
console.log("IMPAR: " + impar)

console.log("---")
